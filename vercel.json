{
  "devCommand": "uvicorn main:app --host 0.0.0.0 --port 3000",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "main.py"
    }
  ],
  "builds": [
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.10" }
    }
  ],
  "engines": {
    "node": "18.x"
  }
}
